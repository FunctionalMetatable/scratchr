<script>
function getUrlVars()
{
	var args = window.location.href.slice(window.location.href.indexOf('?') + 1);
	var vars = [], hash;
	var hashes = args.split('&');
	for(var i = 0; i < hashes.length; i++){
		hash = hashes[i].split('=');
		vars.push(hash[0]);
		vars[hash[0]] = hash[1];
	}
  return vars;
}

var urlvars = getUrlVars();
var sb = urlvars['sb'];
var id =  urlvars['id'];

</script>

